source "Kconfig.zephyr"

config SETTINGS
	default y

config ZMS
	default y if (SOC_FLASH_NRF_RRAM || SOC_FLASH_NRF_MRAM)

config NVS
	default y if !(SOC_FLASH_NRF_RRAM || SOC_FLASH_NRF_MRAM)


config CANBUS_BD
	int  
	default 1000000


#define LOG_LEVEL_NONE 0
#define LOG_LEVEL_ERR  1
#define LOG_LEVEL_WRN  2
#define LOG_LEVEL_INF  3
#define LOG_LEVEL_DBG  4

config CAN_LOG_LVL
	int
	default 4

config BLE_LOG_LVL
	int
	default 4

config PARSE_LOG_LVL
	int
	default 4

config LEDS_LOG_LVL
	int
	default 3

config BUTTONS_LOG_LVL
	int
	default 3
